        span class="note-title" =array_of_notes[7].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[g.to_s]  data-action="click->patterns--midi-device#onPianoKeyClick"   id=g
           
      -g_sharp = array_of_notes[8].first 
