        span class="note-title" =array_of_notes[8].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[g_sharp.to_s]  data-action="click->patterns--midi-device#onPianoKeyClick"   id=g_sharp
      
      -a = array_of_notes[9].first 
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=array_of_notes[9].first
        span class="note-title" =array_of_notes[9].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[a.to_s] data-action="click->patterns--midi-device#onPianoKeyClick"   id=a
      
      -a_sharp = array_of_notes[10].first 
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "grey darken-4"] midi-note-number=array_of_notes[10].first
        span class="note-title" =array_of_notes[10].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[a_sharp.to_s]  data-action="click->patterns--midi-device#onPianoKeyClick"   id=a_sharp

      -b = array_of_notes[11].first      
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=b
        span class="note-title" =array_of_notes[11].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[b.to_s] data-action="click->patterns--midi-device#onPianoKeyClick"   id=b
      
  =render 'octave_buttons'





  

  