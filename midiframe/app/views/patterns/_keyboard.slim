div data-controller="patterns--keyboard" data-target="patterns--midi-device.noteStamps" note-stamps=@pattern.note_stamps.to_json text-stamps=@pattern.text_stamps.to_json data-patterns--keyboard-notes=@midi_notes.to_json  data-notes-in-which-octave-identifier=@notes_in_which_octave_identifier data-patterns--keyboard-position=@current_index_in_notes_array data-patterns--keyboard-final-index=@final_index
  ul class="set z-depth-5"
    -@midi_notes.each_with_index do |array_of_notes, index|
      / if the note is in the default index, add nothing otherwise ass hide 
      - display_class = @current_index_in_notes_array == index ? "" : "hide"
      -c = array_of_notes[0].first
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=c
        span class="note-title" =array_of_notes[0].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[c.to_s] data-action="click->patterns--midi-device#onPianoKeyClick" id=c
      
      -c_sharp = array_of_notes[1].first
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "grey darken-4"] midi-note-number=c_sharp
        span class="note-title" =array_of_notes[1].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[c_sharp.to_s] data-action="click->patterns--midi-device#onPianoKeyClick"  id=c_sharp
      
      -d = array_of_notes[2].first
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=d
        span class="note-title" =array_of_notes[2].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[d.to_s] data-action="click->patterns--midi-device#onPianoKeyClick" id=d
      
      -d_sharp = array_of_notes[3].first
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "grey darken-4"] midi-note-number= d_sharp
        span class="note-title" =array_of_notes[3].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[d_sharp.to_s] data-action="click->patterns--midi-device#onPianoKeyClick"   id=d_sharp
      
      -e = array_of_notes[4].first
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=e
        span class="note-title" =array_of_notes[4].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[e.to_s] data-action="click->patterns--midi-device#onPianoKeyClick"   id=e

      -f = array_of_notes[5].first      
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "white"] midi-note-number=f
        span class="note-title" =array_of_notes[5].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[f.to_s]  data-action="click->patterns--midi-device#onPianoKeyClick"   id=f
      
      -f_sharp = array_of_notes[6].first 
      li data-target="patterns--keyboard.keyBoardKey patterns--midi-device.keyBoardKey" class=[display_class, "grey darken-4"] midi-note-number=f_sharp
        span class="note-title" =array_of_notes[6].last
        br
        br
        input type="text" class="key" value=@pattern.note_stamps[f_sharp.to_s]  data-action="click->patterns--midi-device#onPianoKeyClick"   id=f_sharp
      
      -g = array_of_notes[7].first 
